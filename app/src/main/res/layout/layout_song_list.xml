        style="@style/fl_song_list"
        android:layout_marginLeft="@dimen/dp120"
        android:layout_marginTop="@dimen/dp70"
        android:nextFocusUp="@+id/cb_song">

        <ImageView style="@style/fl_song_list_play_icon" />

        <TextView
            style="@style/fl_song_list_index"
            android:text="1" />

        <LinearLayout
            style="@style/setEnableFocusNO"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                style="@style/song_list"
                android:text="I am yong bin" />

            <ImageView
                style="@style/setEnableFocusNO"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/dp10"
                android:src="@drawable/mv_tag" />

        </LinearLayout>
    </FrameLayout>

