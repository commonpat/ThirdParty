<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clipChildren="false"
                android:clipToPadding="false">
    <SurfaceView
        android:id="@+id/sv_video"
        style="@style/setEnableFocusNO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"/>
    <ImageView
        android:id="@+id/bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitCenter"/>
    <include android:id="@+id/activity_album_default" layout="@layout/activity_album_default_new" android:visibility="visible"/>
    <include android:id="@+id/activity_album_five" layout="@layout/activity_album_five" android:visibility="gone"/>
    <include android:id="@+id/activity_album_four" layout="@layout/activity_album_four" android:visibility="gone"/>




</RelativeLayout>